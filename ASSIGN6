//*                                                          *          00060701
//* PROGRAM FUNCTION:                                        *          00060801
//* THIS PROGRAM USES INSTREAM ASSEMBLER SOURCE CODE TO      *          00060901
//* FIRST ASSEMBLE THE ASSEMBLER SOURCE CODE, THEN BIND      *          00061001
//* AND LASTLY FETCH AND EXECUTE.                            *          00061101
//*                                                          *          00061201
//************************************************************          00061301
//*                                                                     00061401
//************************************************************          00061501
//*                                                          *          00061601
//* JSTEP01 CALLS THE ASSEMBLER PROGRAM MODULE AND ASSEMBLES *          00061701
//* THE ASSEMBLER SOURCE CODE INSTREAM.                      *          00061801
//*                                                          *          00061901
//* DDNAME               FILE DESCRIPTION                    *          00062001
//*                                                          *          00062101
//* SYSLIB     INPUT:  CALLS THE IBM MACRO LIBRARY           *          00062201
//* SYSIN      INPUT:  BEGINS INSTREAM ASSEMBLER SOURCE CODE *          00062301
//* SYSLIN     INPUT:  A TEMPORARY DATA TO PASS TO THE       *          00062401
//*                    BINDER IN JSTEP02                     *          00062501
//* SYSPRINT   OUTPUT: SOURCE LISTING AND DIAGNOSTIC         *          00062601
//*                    MESSAGES                              *          00062701
//* SYSUT1     INPUT:  SCRATCH DATASET FOR ASSEMBLY          *          00062801
//*                                                          *          00062901
//************************************************************          00063001
//*                                                                     00063101
//JSTEP01  EXEC PGM=ASMA90,PARM=ASA                                     00063201
//*                                                                     00063301
//SYSLIB   DD DSN=SYS1.MACLIB,                                          00063401
//            DISP=SHR                                                  00063501
//*                                                                     00063601
//SYSIN    DD *                                                         00063701
         PRINT  NOGEN                                                   00063801
*********************************************************************** 00063900
*                                      	                               *        00064000
* PROGRAM: MAIN                                                       * 00064100
* AUTHOR: MITCH MYERS AND ZUBAIDAH ALQAISI                            * 00064200
* DATE:  10/24/2017                                                   * 00064300
*                                                                     * 00064400
* FUNCTION:  THIS PROGRAM UTILIZES 5 ASSEMBLER QSAM MACROS.           * 00064500
*            THE PROGRAM READS IN TWO 80 BYTE REPORTS, AND            * 00064600
*            MANIPULATES THE NUMERICAL DATA BY PACKING ZONED          * 00064700
*            DECIMALS AND MOVEING THEM TO EDIT LINES.                 * 00064800
*                                                                     * 00064900
* INPUT:     INPUT - THE INPUT DATA RECORDS READ IN FROM DATA5        * 00065000
*                                                                     * 00065100
* OUTPUT:    THIS PROGRAM PRINTS OUT TWO REPORTS, A BROKER SALES      * 00065200
*            COMMISSION REPORT, AND A LARGE DEPOSITORS REPORT.        * 00065300
*            EACH REPORT FEATURES HEADERS THAT DISPLAY THE CURRENT    * 00065400
*            DATE, TIME AND PAGE NUMBER. EACH REPORT PRINTS ONLY      * 00065500
*            15 ENTRIES PER LINE, DOUBLE SPACED.                      * 00065600
*                                                                     * 00065700
*********************************************************************** 00065800
*                                                                       00065900
*********************************************************************** 00066000
*                      BEGIN MAIN ROUTINE                             * 00066100
*********************************************************************** 00066200
*                                                                       00066300
MAIN     CSECT                          BEGIN MAIN                      00066400
*                                                                       00066501
         STM   14,12,12(13)             SAVE CALLER'S REGS              00066600
         LR    12,15                    SET R12 TO R15                  00066700
         USING MAIN,12                  ESTABLISH R12 AS 1ST BASE REG   00066800
         LA    11,4095(,12)             ESTABLISH R11 AS 2ND BASE REG   00066900
         LA    11,1(,11)                LOAD R11 AFTER MAX OF R12       00067000
        
                                                                        00067100
         LA    14,MAINSAVE              R14 -> CURRENT SAVE AREA        00067200
         ST    13,4(,14)                SAVE CALLER'S SAVE AREA ADDR    00067300
         ST    14,8(,14)                SAVE CURRENT SAVE AREA ADDR     00067400
         LR    13,14                    R13 -> CURRENT SAVE AREA        00067500
*                                                                       00067600
*********************************************************************** 00067700
*                      CALLING TIME MACRO                             * 00067800
*********************************************************************** 00067900
*                                                                       00068000
         TIME  DEC,DATETIME,LINKAGE=SYSTEM,DATETYPE=MMDDYYYY            00068100
*                                                                       00068200
*********************************************************************** 00068300
*                      INPUT FILE ROUTINE                             * 00068400
*********************************************************************** 00068500
*                                                                       00068600
         OPEN  (INDCB,(INPUT))          OPEN INPUT FILE                 00068700
         LTR   15,15                    CHECK FOR SUCCESSFUL OPEN       00068800
         BZ    OPENOK1                  BRANCH IF SUCCESSFUL TO OPENOK1 00068900
         ABEND 777,DUMP                 ABEND IF OPEN FAILS DISPLAY 777 00069000
*                                                                       00069156
OPENOK1  OPEN  (OUTDCB,(OUTPUT))        OPEN OUTPUT FILE                00069200
         LTR   15,15                    CHECK FOR SUCCESSFUL OPEN       00069300
         BZ    OPENOK2                  BRANCH IF SUCCESSFUL TO OPENOK2 00069400
         ABEND 888,DUMP                 ABEND IF OPEN FAILS DISPLAY 888 00069500
*                                                                       00069601
OPENOK2  OPEN  (TEMPDCB,(OUTPUT))       OPEN THE TEMPORARY OUTPUT FILE  00069700
         LTR   15,15                    CHECK FOR SUCCESSFUL OPEN       00069800
         BZ    OPENOK3                  BRANCH IF SUCCESSFUL TO OPENOK3 00069900
         ABEND 444,DUMP                 ABEND IF OPEN FAILS DISPLAY 444 00070000
*                                                                       00070100
OPENOK3  GET   INDCB,INBUFFER           READ THE 80 BYTE FUND RECORD    00070200
*                                                                       00070300
         MVC   OFUNDNME(25),IFUNDNME    MOVE THE FUND NAME              00070489
*                                                                       00070589
         PACK  PSHRPRC(3),ISHRPRC(5)    PACK THE SHARE PRICE            00070600
         MVC   OSHRPRC(7),ESHRPRC       MOVE THE EDIT PATTERN TO OUTPUT 00070700
         LA    1,OSHRPRC+3              DEFAULT LOC FOR $0.00           00070899
         EDMK  OSHRPRC(7),PSHRPRC       EDIT THE SHARE PRICE            00070900
         BCTR  1,0                      DECREMENTING REGISTER 1         00071000
         MVI   0(1),C'$'                MOVE $ TO SHARE PRICE           00071100
*                                                                       00071200
         PACK  PCOMPCT1(4),ICOM1(6)     PACK COMMISSION PERCENTAGE 1    00071300
         PACK  PCOMPCT2(4),ICOM2(6)     PACK COMMISSION PERCENTAGE 2    00071400
         PACK  PCOMPCT3(4),ICOM3(6)     PACK COMMISSION PERCENTAGE 3    00071500
*                                                                       00071642
         LA    7,99                     SET REG7 AS COUNTER TO 99       00072300
         LA    8,15                     SET REG8 TO BRANCH COND ON 15   00072400
*                                                                       00072556
         MVC   OUTDATE-2(13),EDATE      EDIT THE DATE PATTERN           00072600
         ED    OUTDATE-2(13),DATETIME+7                                 00072700
         MVC   OUTTIME1-2(11),ETIME     EDIT THE TIME PATTERN           00072900
         ED    OUTTIME1-2(11),DATETIME-1                                00073000
*                                                                       00073100
*********************************************************************** 00073200
*              REPORT 1 INPUT DATA PROCESSING ROUTINE                 * 00073300
*********************************************************************** 00073400
*                                                                       00073500
         GET   INDCB,INDEPREC           READ FIRST DEPOSIT RECORD       00073699
*                                                                       00073799
LOOP1    CLI   EOFFLAG1,C'Y'            TEST FOR EOF                    00073899
         BE    ENDLOOP1                 BRANCH IF EOF                   00073956
*                                                                       00074000
PRNTHDR  CR    7,8                      COMPARE REG7 TO REG 8           00075700
         BL    CONTPRC1                 IF NOT 15 LINES KEEP PROCESSING 00075800
*                                                                       00075900
         AP    PGCOUNT(2),=PL1'1'       ADD 1 TO THE PAGE COUNT         00076000
         MVC   OUTPAGE(4),EPAGE         MOVE PATTERN TO PRINT LINE      00076100
         ED    OUTPAGE(4),PGCOUNT       EDIT THE PAGE COUNTER           00076200
*                                                                       00076800
         PUT   OUTDCB                   PRINT FIRST PAGE HEADER LINE    00077199
         MVC   0(133,1),PRTHDRL1+1      MOVING PGHDR LINE TO PRINT ADDR 00077200
         PUT   OUTDCB                   PRINT 2ND PAGE HEADER LINE      00077400
         MVC   0(133,1),PRTHDRL2+1      MOVING 2ND HDR TO PRINT ADDRESS 00077500
         PUT   OUTDCB                   PRINT 3RD PAGE HEADER LINE      00077600
         MVC   0(133,1),PRTHDRL3        MOVING 3RD HDR TO PRINT ADDR    00077700
         PUT   OUTDCB                   PRINT THE COLUMN HEADERS        00077899
         MVC   0(133,1),COLUMN1         MOVING COLUMN HDR TO PRINT ADDR 00077900
         PUT   OUTDCB                   PRINT THE COLUMN HYPHENS        00078099
         MVC   0(133,1),HYPHEN1         MOVING COLMN HYPH TO PRINT ADDR 00078100
*                                                                       00078200
         SR    7,7                      ZERO REG7 TO RESET COUNTER      00078300
*                                                                       00078400
CONTPRC1 MVC   OBRANME(25),IBRANME      MOVE BRANCH NAME TO OUTPUT      00078500
         MVC   OBRONME(25),IBRONME      MOVE BROKER NAME TO OUTPUT      00078656
*                                                                       00078742
         PACK  PDEPAMT(7),IDEPAMT(13)   PACK DEPOSIT AMOUNT             00078856
         PACK  PCOMFLAG(1),ICOMFLAG(1)  PACK COMMISSION FLAG            00078900
*                                                                       00079041
         LA    1,ODEPAMT + 14           DEFAULT LOCATION FOR $0.00      00079100
         MVC   ODEPAMT(18),EDEPAMT      MOVE THE EDIT PATTERN TO OUTPUT 00079256
         EDMK  ODEPAMT(18),PDEPAMT      EDIT THE DEPOSIT AMOUNT         00079356
         BCTR  1,0                      DECREMENT REGISTER 1 TO 0       00079456
         MVI   0(1),C'$'                MOVE $ TO DEPOSIT AMOUNT        00079556
*                                                                       00079641
         AP    PNUMSALE(2),=PL1'1'      CALCULATING NUMBER OF SALES     00079700
         AP    PTDEPAMT(8),PDEPAMT(7)   CALCULATING DEPOSIT TOTAL AMT   00079800
*                                                                       00079900
         ZAP   PSHRAMT(12),PDEPAMT(7)   MOVE TO LARGER FIELD            00080000
         SRP   PSHRAMT(12),4,0          SHIFT ROUND PACK BY 4 DIGITS    00080100
         DP    PSHRAMT(12),PSHRPRC(3)   CALCULATING SHARE AMOUNT        00080200
         SRP   PSHRAMT(9),64-1,5        SHIFT AND ROUND PACK            00080300
         SRP   PSHRAMT(9),1,0           SHIFT AND ROUND PACK BY 1 DIGIT 00080400
         MVC   OSHRAMT(23),ESHRAMT      MOVE PATTERN TO OUTPUT          00080500
         ED    OSHRAMT(23),PSHRAMT      EDIT THE SHARE AMOUNT           00080600
*                                                                       00080741
IF0      CP    PSHRAMT(9),PTHRSHLD(6)   COMPARE FOR THRESHOLD           00080800
         BL    ENDIF0                                                   00080900
         MVC   OTBRKNME(25),IBRONME     MOVE BROKER NAME TO OUTPUT      00081000
         ZAP   OTPSHRAM(9),PSHRAMT(9)   MOVE PATTERN TO OUTPUT          00081100
*                                                                       00081200
         PUT   TEMPDCB,TEMPREC          MOVE DATA INTO TEMP FILE        00081300
*                                                                       00081400
ENDIF0   AP    PTSHRAMT(13),PSHRAMT(9)  CALCULATE SHARE AMOUNT TOTAL    00081500
*                                                                       00081600
         ZAP   PCOMAMT(11),PDEPAMT(7)   MOVE TO LARGER FIELD            00081700
*                                                                       00081800
IF1      CP    PCOMFLAG,=PL1'1'         COMPARE COMMISSION FLAG         00081900
         BNE   IF2                      BRANCH TO IF2 IF NOT EQUAL TO 1 00082000
         MP    PCOMAMT(11),PCOMPCT1(4)  MULTIPLY COMM AMOUNT BY COUNT   00082100
         B     ENDIF                    END IF                          00082200
*                                                                       00082300
IF2      CP    PCOMFLAG,=P'2'           COMPARE COMMISSION FLAG         00082400
         BNE   IF3                      BRANCH TO IF3 IF NOT EQUAL TO 2 00082500
         MP    PCOMAMT(11),PCOMPCT2(4)  MULTIPLY COMM AMOUNT BY COUNT   00082600
         B     ENDIF                    END IF                          00082700
*                                                                       00082800
IF3      MP    PCOMAMT(11),PCOMPCT3(4)  MULTIPLY COMM AMOUNT BY COUNT   00082900
*                                                                       00083000
ENDIF    SRP   PCOMAMT(11),64-5,5       SHIFT AND ROUND PACK            00083100
*                                                                       00083200
         LA    1,OCOMAMT+14             DEFAULT LOCATION FRO $0.00      00083300
         MVC   OCOMAMT(18),ECOMAMT      MOVE TO EDIT FIELD              00083400
         EDMK  OCOMAMT(18),PCOMAMT+4    EDIT THE COMMISSION OUTPUT      00083500
         BCTR  1,0                      DECREMENTING REGISTER 1         00083600
         MVI   0(1),C'$'                MOVE $ TO COMMISSION AMOUNT     00083700
*                                                                       00083800
         AP    PTCOMAMT(12),PCOMAMT(11) CALCULATING COMM AMOUNT TOTAL   00083900
*                                                                       00084000
         PUT   OUTDCB                   WRITE THE PRINT DATA TO OUTPUT  00084199
         MVC   0(133,1),DETAIL1         MOVING PRNT DATA TO PRNT ADDR   00084200
*                                                                       00084400
         LA    7,1(,7)                  INCREMENT REG7 COUNTER BY 1     00084500
*                                                                       00084617
         GET   INDCB,INDEPREC           GET THE NEXT DEPOSIT RECORD     00084700
*                                                                       00084800
         B     LOOP1                    BACK TO LOOP1                   00084999
*                                                                       00085000
ENDLOOP1 CLOSE (INDCB)                  CLOSE THE INPUT DCB             00085100
*                                                                       00085200
         AP    PGCOUNT(2),=PL1'1'       ADD 1 TO THE PAGE COUNT         00085300
         MVC   OUTPAGE(4),EPAGE         MOVE PATTERN TO PRINT LINE      00085400
         ED    OUTPAGE(4),PGCOUNT       EDIT THE PAGE COUNTER           00085500
*                                                                       00085600
         MVC   ONUMSALE(4),ENUMSALE     MOVE PATTERN TO OUTPUT          00085700
         ED    ONUMSALE(4),PNUMSALE     EDIT NUMBER OF SALES            00085800
*                                                                       00085900
         MVC   OUTTIME2-2(11),ETIME     EDIT THE DATE PATTERN           00086000
         ED    OUTTIME2-2(11),DATETIME-1                                00086100
*                                                                       00086200
         PUT   OUTDCB                   PRINT FIRST PAGE HEADER LINE    00086399
         MVC   0(133,1),PRTHDRL1+1      PRNT 1ST PAGE TO PRINT ADDR     00086400
         PUT   OUTDCB                   PRINT 4TH PAGE HEADER LINE      00086500
         MVC   0(133,1),PRTHDRL4+1      MOVING 4TH PG HDR TO PRNT ADDR  00086600
         PUT   OUTDCB                   PRINT 3RD PAGE HEADER LINE      00086700
         MVC   0(133,1),PRTHDRL3        MOVING 3RD PGE HDR TO PRT ADDR  00086800
         PUT   OUTDCB                   PRINT THE COLUMN HEADERS        00086999
         MVC   0(133,1),COLUMN2         MOVING COLUMN HDR TO PRINT ADDR 00087099
         PUT   OUTDCB                   PRINT THE COLUMN HYPHENS        00087199
         MVC   0(133,1),HYPHEN2         MOVING COLMN HYPH TO PRINT ADDR 00087299
*                                                                       00087300
         LA    1,OTDEPAMT+17            DEFAULT LOCATION FOR $0.00      00087400
         MVC   OTDEPAMT(21),ETDEPAMT    MOVE PATTERN TO PRINT LINE      00087500
         EDMK  OTDEPAMT(21),PTDEPAMT    EDIT THE TOTAL DEPOSIT AMOUNT   00087600
         BCTR  1,0                      DECREMENT REGISTER 1            00087700
         MVI   0(1),C'$'                MOVE $ TO TOTAL DEPOSIT AMOUNT  00087800
*                                                                       00087900
         MVC   OTSHRAMT(34),ETSHRAMT    MOVE PATTERN TO PRINT LINE      00088000
         ED    OTSHRAMT(34),PTSHRAMT    EDIT THE TOTAL SHARE AMOUNT     00088199
*                                                                       00088200
         LA    1,OTCOMAMT+27            DEFAULT LOCATION FOR $0.00      00088300
         MVC   OTCOMAMT(31),ETCOMAMT    MOVE PATTERN TO PRINT LINE      00088400
         EDMK  OTCOMAMT(31),PTCOMAMT    EDIT THE TOTAL COMM AMOUNT      00088500
         BCTR  1,0                      DECREMENT REGISTER 1            00088600
         MVI   0(1),C'$'                MOVE $ TO TOTAL COMM AMOUNT     00088700
*                                                                       00088800
         PUT   OUTDCB                   WRITE THE NEXT TOTALS RECORD    00088999
         MVC   0(133,1),DETAIL2        MOVE NEXT TOT TO PRINT ADDRESS   00089000
*                                                                       00089300
*********************************************************************** 00089400
*                   TEMPORARY DCB STORAGE ROUTINE                     * 00089500
*********************************************************************** 00089600
*                                                                       00089700
         CLOSE (INDCB,,TEMPDCB)         CLOSE TEMP AND INPUT FILES      00089800
*                                                                       00089900
         OPEN  (TEMPDCB,(INPUT))        OPEN INPUT FILE                 00090000
         LTR   15,15                    CHECK FOR SUCCESSFUL OPEN       00090100
         BZ    OPENOK4                  BRANCH IF SUCCESSFUL OPENOK4    00090200
         ABEND 555,DUMP                 ABEND IF OPEN FAILS DISPLAY 555 00090300
*                                                                       00090400
OPENOK4  ZAP   PGCOUNT(2),=PL1'0'       ZERO OUT THE PAGE COUNTER       00090599
*                                                                       00090600
         LA    7,99                     SET REG7 AS COUNTER TO 99       00090800
*                                                                       00090900
         MVC   OUTTIME3-2(11),ETIME     EDIT THE TIME PATTERN           00091000
         ED    OUTTIME3-2(11),DATETIME-1                                00091100
*                                                                       00091200
*********************************************************************** 00091300
*              REPORT 2 INPUT DATA PROCESSING ROUTINE                 * 00091400
*********************************************************************** 00091500
*                                                                       00091600
         GET   TEMPDCB                  GET FIRST RECORD IN TEMP FILE   00091799
*                                                                       00091899
LOOP2    CLI   EOFFLAG2,C'Y'            CHECK FOR EOF                   00091999
         BE    ENDLOOP2                 BRANCH ENDLOOP2 IF NOT EQUAL    00092000
         MVC   TEMPREC(34),0(1)         MOVE THE 34 BYTES TO TEMP REC   00092199
*                                                                       00092200
PRNTHDR2 CR    7,8                      COMPARE REG7 TO REG 8           00092300
         BL    CONTPRC2                 IF NOT 15 LINES KEEP PROCESSING 00092400
*                                                                       00092500
         AP    PGCOUNT(2),=PL1'1'       ADD 1 TO THE PAGE COUNT         00092600
         MVC   OUTPAGE(4),EPAGE         MOVE PATTERN TO PRINT LINE      00092700
         ED    OUTPAGE(4),PGCOUNT       EDIT THE PAGE COUNTER           00092800
*                                                                       00092900
         PUT   OUTDCB                   PRINT FIRST PAGE HEADER LINE    00093099
         MVC   0(133,1),PRTHDRL1+1      MOVE 1ST PAGE HDR TO PRINT ADDR 00093100
         PUT   OUTDCB                   PRINT 5TH PAGE HEADER LINE      00093200
         MVC   0(133,1),PRTHDRL5+1      MOVE 5TH PAGE HDR TO PRINT ADDR 00093300
         PUT   OUTDCB                   PRINT THE COLUMN HEADERS        00093499
         MVC   0(133,1),COLUMN3         MOVE COLUMN HDRS TO PRINT ADDR  00093599
         PUT   OUTDCB                   PRINT THE COLUMN HYPHENS        00093699
         MVC   0(133,1),HYPHEN3         MOVE COUMN HYPHEN TO PRINT ADDR 00093799
*                                                                       00093800
         SR    7,7                      ZERO REG7 TO RESET COUNTER      00093900
*                                                                       00094000
CONTPRC2 MVC   OBRONME2(25),OTBRKNME    MOVE BROKER NAME TO OUTPUT      00094100
         MVC   OSHRAMT2(23),ESHRAMT     MOVE PATTERN OF SHARE AMOUNT    00094200
         ED    OSHRAMT2(23),OTPSHRAM    EDIT TEMP SHARE AMOUNT          00094300
*                                                                       00094400
         AP    PTSALNUM(2),=PL1'1'      ADD 1 TO TEMP SALES NUMBER      00094500
         AP    PTSHRTMP(10),OTPSHRAM(9) CALCULATING TEMP TOTAL SHARES   00094600
*                                                                       00094700
         PUT   OUTDCB                   PRINT TEMPORARY RECORD          00094899
         MVC   0(133,1),DETAIL3         MOVE TEMP REC TO PRINT ADDRESS  00094999
*                                                                       00095100
         LA    7,1(,7)                  INCREMENT REG7 COUNTER BY 1     00095200
*                                                                       00095300
         GET   TEMPDCB                  GET THE NEXT TEMP RECORD        00095499
*                                                                       00095500
         B     LOOP2                    BRANCH TO TOP OF LOOP2          00095600
*                                                                       00095700
ENDLOOP2 MVC   ONUMSALE(4),ENUMSALE     MOVE PATTERN TO OUTPUT          00095800
         ED    ONUMSALE(4),PNUMSALE     EDIT NUMBER OF SALES            00095900
*                                                                       00096000
         MVC   ONUMSAL2(4),ETNUMSA2     MOVE THE PATERN TO PRINT LINE   00096100
         ED    ONUMSAL2(4),PTSALNUM     EDIT NUMBER OF SALES IN TEMP    00096200
*                                                                       00096300
         MVC   OTSRAMT2(25),ETSRAMT2    MOVE PATTERN TO PRINT LINE      00096400
         ED    OTSRAMT2(25),PTSHRTMP    EDIT TOTAL SHARE SOLD IN TEMP   00096500
*                                                                       00096600
         MVC   OUTTIME4-2(11),ETIME     EDIT THE TIME PATTERN           00096700
         ED    OUTTIME4-2(11),DATETIME-1                                00096800
*                                                                       00096900
         AP    PGCOUNT(2),=PL1'1'       ADD 1 TO THE PAGE COUNT         00097000
         MVC   OUTPAGE(4),EPAGE         MOVE PATTERN TO PRINT LINE      00097100
         ED    OUTPAGE(4),PGCOUNT       EDIT THE PAGE COUNTER           00097200
*                                                                       00097300
         PUT   OUTDCB                   PRINT FIRST PAGE HEADER LINE    00097499
         MVC   0(133,1),PRTHDRL1+1      MOVE 1ST PAGE HDR TO PRINT ADDR 00097500
         PUT   OUTDCB                   PRINT 6TH PAGE HEADER LINE      00097600
         MVC   0(133,1),PRTHDRL6+1      MOVE 6TH PAGE HDR TO PRINT ADDR 00097700
         PUT   OUTDCB                   PRINT THE COLUMN HEADERS        00097899
         MVC   0(133,1),COLUMN4         MOVE COLUMN HDRS TO PRINT ADDR  00097999
         PUT   OUTDCB                   PRINT THE COLUMN HYPHENS        00098099
         MVC   0(133,1),HYPHEN4         MOVE COLMN HYPHEN TO PRINT ADDR 00098199
*                                                                       00098200
         PUT   OUTDCB                   PRINT SECOND TEMP REPORT        00098399
         MVC   0(133,1),DETAIL4         MOVE 2ND TEMP REP TO PRINT ADDR 00098400
*                                                                       00098500
*********************************************************************** 00098600
*                   CLOSE OUTPUT RECORD ROUTINE                       * 00098700
*********************************************************************** 00098800
*                                                                       00098900
         CLOSE (OUTDCB,,TEMPDCB)        CLOSE OUTPUT AND TEMP FILE      00099000
*                                                                       00099100
         L     13,4(,13)                R13 -> CALLER'S SAVE AREA       00099200
         L     14,12(,13)               RESTORE R14                     00099300
         LM    0,12,20(13)             RESTORE R0 THROUGH R12           00099400
*                                                                       00099517
         BR    14                       RETURN TO CALLER                00099600
*                                                                       00099717
*********************************************************************** 00099800
*                 LITERAL POOL AND PROGRAM STORAGE                    * 00099900
*********************************************************************** 00100000
*                                                                       00100100
         LTORG                          LTORG TO ORGANIZE LITERALS      00100200
*                                                                       00100317
         ORG   MAIN+((*-MAIN+31)/32)*32                                 00100417
         DC    C'HERE IS THE STORAGE FOR THIS PGM'                      00100517
*                                                                       00100600
*********************************************************************** 00100700
*                       PROGRAM STORAGE AREA                          * 00100800
*********************************************************************** 00100900
*                                                                       00101017
MAINSAVE DC    18F'-1'                  MAINREGS FOR STANDARD LINKAGE   00101100
*                                                                       00101217
PSHRPRC  DC    PL3'0'                   PACKED SHARE PRICE              00101300
PDEPAMT  DC    PL7'0'                   PACKED DEPOSIT AMOUNT           00101400
PSHRAMT  DC    PL12'0'                  PACKED SHARE AMOUNT             00101500
PCOMFLAG DC    PL1'0'                   PACKED COMMISSION FLAG          00101600
PCOMAMT  DC    PL11'0'                  PACKED COMMISSION AMOUNT        00101700
PCOMPCT1 DC    PL4'0'                   PACKED COMMISSION PERCENT 1     00101800
PCOMPCT2 DC    PL4'0'                   PACKED COMMISSION PERCENT 2     00101900
PCOMPCT3 DC    PL4'0'                   PACKED COMMISSION PERCENT 3     00102000
PNUMSALE DC    PL2'0'                   PACKED NUMBER OF SALES          00102100
PTDEPAMT DC    PL8'0'                   PACKED TOTAL FOR DEPOSIT AMOUNT 00102200
PTSHRAMT DC    PL13'0'                  PACKED TOTAL FOR SHARE AMOUNT   00102300
PTCOMAMT DC    PL12'0'                  PACKED TOTAL FRO COMM AMOUNT    00102400
PGCOUNT  DC    PL2'0'                   PACKED PAGE COUNTER             00102500
PTHRSHLD DC    PL6'2500000000'          PACKED THRESHOLD STORAGE        00102600
PTSALNUM DC    PL2'0'                   PACKED TEMP NUMBER OF SALES     00102700
PTSHRTMP DC    PL10'0'                  PACKED TEMP TOTAL SHARE SOLD    00102800
*                                                                       00102900
INBUFFER DC    0C                       INPUT BUFFER WITH VALUE X       00103000
ISHRPRC  DS    ZL5                      INPUT SHARE PRICE               00103100
IFUNDNME DS    CL25                     INPUT FUND NAME                 00103200
ICOM1    DS    ZL6                      COMMISSION PERCENT 1            00103300
