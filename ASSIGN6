*********************************************************************** 00123100
*                           DCB STORAGE                               * 00123200
*********************************************************************** 00123300
*                                                                       00123400
*********************************************************************** 00123500
*                          INPUT DCB MACRO                            * 00123600
*********************************************************************** 00123700
*                                                                       00123800
INDCB    DCB   DDNAME=MAININ,                                          X00123942
               DEVD=DA,                                                X00124042
               DSORG=PS,                                               X00124142
               MACRF=GM,                                               X00124242
               RECFM=FB,                                               X00124342
               LRECL=80,                                               X00124442
               EODAD=EOFRTN1                                            00124599
*                                                                       00124642
*  INPUT DCB'S EOF FLAG AND EOF ROUTINE                                 00124742
*                                                                       00124842
EOFFLAG1 DC    C'N'                    EOFFLAG FOR INPUT FILE           00124999
*                                                                       00125042
EOFRTN1  MVI   EOFFLAG1,C'Y'           SET EOF FLAG TO Y                00125199
         BR    14                      BRANCH TO STMT FOLLOWING GET     00125200
*                                                                       00125300
*********************************************************************** 00125400
*                        OUTPUT DCB MACRO                             * 00125500
*********************************************************************** 00125600
*                                                                       00125700
OUTDCB   DCB   DDNAME=MAINOUT,                                         X00125842
               DEVD=DA,                                                X00125942
               DSORG=PS,                                               X00126042
               MACRF=PL,                                               X00126199
               RECFM=FBA,                                              X00126242
               LRECL=133,                                              X00126342
               BLKSIZE=133                                              00126442
*                                                                       00126500
*********************************************************************** 00126600
*                       TEMPORARY DCB MACRO                           * 00126700
*********************************************************************** 00126800
*                                                                       00126900
TEMPDCB  DCB   DDNAME=TEMP,                                            X00127000
               DEVD=DA,                                                X00127100
               DSORG=PS,                                               X00127200
               MACRF=(PM,GL),                                          X00127399
               RECFM=FB,                                               X00127400
               LRECL=34,                                               X00127500
               BLKSIZE=340,                                            X00127600
               EODAD=EOFRTN2                                            00127799
*                                                                       00127800
*********************************************************************** 00127900
*                         EOF FLAG ROUTINE                            * 00128000
*********************************************************************** 00128100
*                                                                       00128200
EOFFLAG2 DC    C'N'                    EOFFLAG FOR TEMP FILE            00128399
*                                                                       00128499
EOFRTN2  MVI   EOFFLAG2,C'Y'           SET EOF FLAG TO Y                00128599
         BR    14                      BRANCH TO STMT FOLLOWING GET     00128699
*                                                                       00128700
*********************************************************************** 00128800
*                       STANDARD EXIT LINKAGE                         * 00128900
*********************************************************************** 00129000
*                                                                       00129100
         SR    15,15                   RC = 0                           00129200
         L     13,4(,13)               R13 -> CALLER'S SAVE AREA        00129300
         L     14,12(,13)              RESTORE R14                      00129400
         LM    0,12,20(13)             RESTORE R0 THROUGH R12           00129500
         BM    14                      RETURN TO CALLER                 00129600
*                                                                       00129700
*********************************************************************** 00129800
*                         END MAIN ROUTINE                            * 00129900
*********************************************************************** 00130000
*                                                                       00130100
         END   MAIN                                                     00130242
